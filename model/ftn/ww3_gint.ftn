                  BEDFORMS(1:NSEA,2) = AUX2
!
                ELSE IF ( IOGRP .EQ. 7 .AND. IGRPP .EQ. 4 ) THEN
!
! Energy dissipation in WBBL
!
                  AUX1 = UNDEF
		  DO ISEA = 1, NSEA
		    IF ( GR_WTS(ISEA)%NO .NE. 0 ) THEN
		      DO J = 1,GR_WTS(ISEA)%NO
		        IG = GR_WTS(ISEA)%ID(J)
		        DO K = 1,GR_INTS(IG)%COUNT(ISEA)
		  	  GSEA = GR_INTS(IG)%ID(ISEA,K)
			  VAR1 = WADATS(IG)%PHIBBL(GSEA)
			  IF ( VAR1 .NE. UNDEF ) THEN
			    IF ( AUX1(ISEA) .EQ. UNDEF ) THEN
			      AUX1(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ELSE
			      AUX1(ISEA)=AUX1(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ENDIF
			  ENDIF
		        END DO
		      END DO
		    ENDIF
		  END DO
		  DO ISEA = 1,NO_WTC
		    GSEA = NO_WTS(ISEA)%NO
		    AUX1(GSEA) = UNDEF
		  END DO
                  PHIBBL = AUX1
!
                ELSE IF ( IOGRP .EQ. 7 .AND. IGRPP .EQ. 5 ) THEN
!
! Momentum dissipation in WBBL
!
                  AUX1 = UNDEF
                  AUX2 = UNDEF
		  DO ISEA = 1, NSEA
		    IF ( GR_WTS(ISEA)%NO .NE. 0 ) THEN
		      DO J = 1,GR_WTS(ISEA)%NO
		        IG = GR_WTS(ISEA)%ID(J)
		        DO K = 1,GR_INTS(IG)%COUNT(ISEA)
		  	  GSEA = GR_INTS(IG)%ID(ISEA,K)
			  VAR1 = WADATS(IG)%TAUBBL(GSEA,1)
			  VAR2 = WADATS(IG)%TAUBBL(GSEA,2)
			  IF ( VAR1 .NE. UNDEF ) THEN
			    IF ( AUX1(ISEA) .EQ. UNDEF ) THEN
			      AUX1(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                     GR_WTS(ISEA)%WT(J)*VAR1 
			      AUX2(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                     GR_WTS(ISEA)%WT(J)*VAR2 
			    ELSE
			      AUX1(ISEA)=AUX1(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                     GR_WTS(ISEA)%WT(J)*VAR1 
			      AUX2(ISEA)=AUX2(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                     GR_WTS(ISEA)%WT(J)*VAR2 
			    ENDIF
			  ENDIF
		        END DO
		      END DO
		    ENDIF
		  END DO
		  DO ISEA = 1,NO_WTC
		    GSEA = NO_WTS(ISEA)%NO
		    AUX1(GSEA) = UNDEF
		    AUX2(GSEA) = UNDEF
		  END DO
                  TAUBBL(1:NSEA,1) = AUX1
                  TAUBBL(1:NSEA,2) = AUX2
!
                ELSE IF ( IOGRP .EQ. 8 .AND. IGRPP .EQ. 1 ) THEN
!
! Mean Square Slope
!
                  AUX1 = UNDEF
                  AUX2 = UNDEF
		  DO ISEA = 1, NSEA
		    IF ( GR_WTS(ISEA)%NO .NE. 0 ) THEN
		      DO J = 1,GR_WTS(ISEA)%NO
		        IG = GR_WTS(ISEA)%ID(J)
		        DO K = 1,GR_INTS(IG)%COUNT(ISEA)
		  	  GSEA = GR_INTS(IG)%ID(ISEA,K)
			  VAR1 = WADATS(IG)%MSSX(GSEA)
			  VAR2 = WADATS(IG)%MSSY(GSEA)
			  IF ( VAR1 .NE. UNDEF ) THEN
			    IF ( AUX1(ISEA) .EQ. UNDEF ) THEN
			      AUX1(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                     GR_WTS(ISEA)%WT(J)*VAR1 
			      AUX2(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                     GR_WTS(ISEA)%WT(J)*VAR2 
			    ELSE
			      AUX1(ISEA)=AUX1(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                     GR_WTS(ISEA)%WT(J)*VAR1 
			      AUX2(ISEA)=AUX2(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                     GR_WTS(ISEA)%WT(J)*VAR2 
			    ENDIF
			  ENDIF
		        END DO
		      END DO
		    ENDIF
		  END DO
		  DO ISEA = 1,NO_WTC
		    GSEA = NO_WTS(ISEA)%NO
		    AUX1(GSEA) = UNDEF
		    AUX2(GSEA) = UNDEF
		  END DO
                  MSSX = AUX1
                  MSSY = AUX2
!
                ELSE IF ( IOGRP .EQ. 8 .AND. IGRPP .EQ. 2 ) THEN
!
! Spectral tail levels
!
                  AUX1 = UNDEF
                  AUX2 = UNDEF
		  DO ISEA = 1, NSEA
		    IF ( GR_WTS(ISEA)%NO .NE. 0 ) THEN
		      DO J = 1,GR_WTS(ISEA)%NO
		        IG = GR_WTS(ISEA)%ID(J)
		        DO K = 1,GR_INTS(IG)%COUNT(ISEA)
		  	  GSEA = GR_INTS(IG)%ID(ISEA,K)
			  VAR1 = WADATS(IG)%MSCX(GSEA)
			  VAR2 = WADATS(IG)%MSCY(GSEA)
			  IF ( VAR1 .NE. UNDEF ) THEN
			    IF ( AUX1(ISEA) .EQ. UNDEF ) THEN
			      AUX1(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                     GR_WTS(ISEA)%WT(J)*VAR1 
			      AUX2(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                     GR_WTS(ISEA)%WT(J)*VAR2 
			    ELSE
			      AUX1(ISEA)=AUX1(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                     GR_WTS(ISEA)%WT(J)*VAR1 
			      AUX2(ISEA)=AUX2(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                     GR_WTS(ISEA)%WT(J)*VAR2 
			    ENDIF
			  ENDIF
		        END DO
		      END DO
		    ENDIF
		  END DO
		  DO ISEA = 1,NO_WTC
		    GSEA = NO_WTS(ISEA)%NO
		    AUX1(GSEA) = UNDEF
		    AUX2(GSEA) = UNDEF
		  END DO
                  MSCX = AUX1
                  MSCY = AUX2
!
                ELSE IF ( IOGRP .EQ. 9 .AND. IGRPP .EQ. 1 ) THEN
!
! Average time step in integration
!
                  AUX1 = UNDEF
		  DO ISEA = 1, NSEA
		    IF ( GR_WTS(ISEA)%NO .NE. 0 ) THEN
		      DO J = 1,GR_WTS(ISEA)%NO
		        IG = GR_WTS(ISEA)%ID(J)
		        DO K = 1,GR_INTS(IG)%COUNT(ISEA)
		  	  GSEA = GR_INTS(IG)%ID(ISEA,K)
			  VAR1 = WADATS(IG)%DTDYN(GSEA)
			  IF ( VAR1 .NE. UNDEF ) THEN
			    IF ( AUX1(ISEA) .EQ. UNDEF ) THEN
			      AUX1(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ELSE
			      AUX1(ISEA)=AUX1(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ENDIF
			  ENDIF
		        END DO
		      END DO
		    ENDIF
		  END DO
		  DO ISEA = 1,NO_WTC
		    GSEA = NO_WTS(ISEA)%NO
		    AUX1(GSEA) = UNDEF
		  END DO
                  DTDYN = AUX1
!
                ELSE IF ( IOGRP .EQ. 9 .AND. IGRPP .EQ. 2 ) THEN
!
! Cut-off frequency
!
                  AUX1 = UNDEF
		  DO ISEA = 1, NSEA
		    IF ( GR_WTS(ISEA)%NO .NE. 0 ) THEN
		      DO J = 1,GR_WTS(ISEA)%NO
		        IG = GR_WTS(ISEA)%ID(J)
		        DO K = 1,GR_INTS(IG)%COUNT(ISEA)
		  	  GSEA = GR_INTS(IG)%ID(ISEA,K)
			  VAR1 = WADATS(IG)%FCUT(GSEA)
			  IF ( VAR1 .NE. UNDEF ) THEN
			    IF ( AUX1(ISEA) .EQ. UNDEF ) THEN
			      AUX1(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ELSE
			      AUX1(ISEA)=AUX1(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ENDIF
			  ENDIF
		        END DO
		      END DO
		    ENDIF
		  END DO
		  DO ISEA = 1,NO_WTC
		    GSEA = NO_WTS(ISEA)%NO
		    AUX1(GSEA) = UNDEF
		  END DO
                  FCUT = AUX1
!
                ELSE IF ( IOGRP .EQ. 9 .AND. IGRPP .EQ. 3 ) THEN
!
! Max. CFL number for spatial advection
!
                  AUX1 = UNDEF
		  DO ISEA = 1, NSEA
		    IF ( GR_WTS(ISEA)%NO .NE. 0 ) THEN
		      DO J = 1,GR_WTS(ISEA)%NO
		        IG = GR_WTS(ISEA)%ID(J)
		        DO K = 1,GR_INTS(IG)%COUNT(ISEA)
		  	  GSEA = GR_INTS(IG)%ID(ISEA,K)
			  VAR1 = WADATS(IG)%CFLXYMAX(GSEA)
			  IF ( VAR1 .NE. UNDEF ) THEN
			    IF ( AUX1(ISEA) .EQ. UNDEF ) THEN
			      AUX1(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ELSE
			      AUX1(ISEA)=AUX1(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ENDIF
			  ENDIF
		        END DO
		      END DO
		    ENDIF
		  END DO
		  DO ISEA = 1,NO_WTC
		    GSEA = NO_WTS(ISEA)%NO
		    AUX1(GSEA) = UNDEF
		  END DO
                  CFLXYMAX = AUX1
!
                ELSE IF ( IOGRP .EQ. 9 .AND. IGRPP .EQ. 4 ) THEN
!
! Max. CFL number for theta advection
!
                  AUX1 = UNDEF
		  DO ISEA = 1, NSEA
		    IF ( GR_WTS(ISEA)%NO .NE. 0 ) THEN
		      DO J = 1,GR_WTS(ISEA)%NO
		        IG = GR_WTS(ISEA)%ID(J)
		        DO K = 1,GR_INTS(IG)%COUNT(ISEA)
		  	  GSEA = GR_INTS(IG)%ID(ISEA,K)
			  VAR1 = WADATS(IG)%CFLTHMAX(GSEA)
			  IF ( VAR1 .NE. UNDEF ) THEN
			    IF ( AUX1(ISEA) .EQ. UNDEF ) THEN
			      AUX1(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ELSE
			      AUX1(ISEA)=AUX1(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ENDIF
			  ENDIF
		        END DO
		      END DO
		    ENDIF
		  END DO
		  DO ISEA = 1,NO_WTC
		    GSEA = NO_WTS(ISEA)%NO
		    AUX1(GSEA) = UNDEF
		  END DO
                  CFLTHMAX = AUX1
!
                ELSE IF ( IOGRP .EQ. 9 .AND. IGRPP .EQ. 5 ) THEN
!
! Max. CFL number for k advection
!
                  AUX1 = UNDEF
		  DO ISEA = 1, NSEA
		    IF ( GR_WTS(ISEA)%NO .NE. 0 ) THEN
		      DO J = 1,GR_WTS(ISEA)%NO
		        IG = GR_WTS(ISEA)%ID(J)
		        DO K = 1,GR_INTS(IG)%COUNT(ISEA)
		  	  GSEA = GR_INTS(IG)%ID(ISEA,K)
			  VAR1 = WADATS(IG)%CFLKMAX(GSEA)
			  IF ( VAR1 .NE. UNDEF ) THEN
			    IF ( AUX1(ISEA) .EQ. UNDEF ) THEN
			      AUX1(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ELSE
			      AUX1(ISEA)=AUX1(ISEA) + GR_INTS(IG)%WT(ISEA,K)* &
			                   GR_WTS(ISEA)%WT(J)*VAR1 
			    ENDIF
			  ENDIF
		        END DO
		      END DO
		    ENDIF
		  END DO
		  DO ISEA = 1,NO_WTC
		    GSEA = NO_WTS(ISEA)%NO
		    AUX1(GSEA) = UNDEF
		  END DO
                  CFLKMAX = AUX1
!
                ELSE IF ( IOGRP .EQ. 10 .AND. IGRPP .EQ. 1 ) THEN
!
! User defined variable 1 (we are assuming they are scalars)
!
                  AUX1 = UNDEF
		  DO ISEA = 1, NSEA
		    IF ( GR_WTS(ISEA)%NO .NE. 0 ) THEN
		      DO J = 1,GR_WTS(ISEA)%NO
		        IG = GR_WTS(ISEA)%ID(J)
		        DO K = 1,GR_INTS(IG)%COUNT(ISEA)
		  	  GSEA = GR_INTS(IG)%ID(ISEA,K)
			  VAR1 = WADATS(IG)%USERO(GSEA,1)
			  IF ( VAR1 .NE. UNDEF ) THEN
		            IF ( AUX1(ISEA) .EQ. UNDEF ) THEN
			      AUX1(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                    GR_WTS(ISEA)%WT(J)*VAR1 
			    ELSE
			      AUX1(ISEA) = AUX1(ISEA) +                       &
                                                GR_INTS(IG)%WT(ISEA,K)*       &
			                        GR_WTS(ISEA)%WT(J)*VAR1 
			    ENDIF
			  ENDIF
		        END DO
		      END DO
		    ENDIF
		  END DO
		  DO ISEA = 1,NO_WTC
		    GSEA = NO_WTS(ISEA)%NO
		    AUX1(GSEA) = UNDEF
		  END DO
                  USERO(1:NSEA,1) = AUX1
!
                ELSE IF ( IOGRP .EQ. 10 .AND. IGRPP .EQ. 2 ) THEN
!
! User defined variable 2 (we are assuming they are scalars)
!
                  AUX1 = UNDEF
		  DO ISEA = 1, NSEA
		    IF ( GR_WTS(ISEA)%NO .NE. 0 ) THEN
		      DO J = 1,GR_WTS(ISEA)%NO
		        IG = GR_WTS(ISEA)%ID(J)
		        DO K = 1,GR_INTS(IG)%COUNT(ISEA)
		  	  GSEA = GR_INTS(IG)%ID(ISEA,K)
			  VAR1 = WADATS(IG)%USERO(GSEA,2)
			  IF ( VAR1 .NE. UNDEF ) THEN
		            IF ( AUX1(ISEA) .EQ. UNDEF ) THEN
			      AUX1(ISEA) = GR_INTS(IG)%WT(ISEA,K)*            &
			                    GR_WTS(ISEA)%WT(J)*VAR1 
			    ELSE
			      AUX1(ISEA) = AUX1(ISEA) +                       &
                                                GR_INTS(IG)%WT(ISEA,K)*       &
			                        GR_WTS(ISEA)%WT(J)*VAR1 
			    ENDIF
			  ENDIF
		        END DO
		      END DO
		    ENDIF
		  END DO
		  DO ISEA = 1,NO_WTC
		    GSEA = NO_WTS(ISEA)%NO
		    AUX1(GSEA) = UNDEF
		  END DO
                  USERO(1:NSEA,2) = AUX1
!
                ELSE 
!
! End of output options
!
	          WRITE(NDSE,1003) IOGRP, IGRPP
!
                END IF
              END IF
           END DO
        END DO
!
!------------------------------------------------------------------------------
! 3. Write out interpolated data to target output file
!
	CALL W3IOGO('WRITE',FIDOUT(NG),IOTST,NG)
!
	RETURN
!
! Error escape locations
!
 1003   FORMAT (/' *** ERROR IN WAVEGRID_INTERP *** '/                        &
	         '   OUTPUT NOT DEFINED FOR (GROUP, PARAM): ',I5,I5/)
!/
!/ End of W3EXGI ------------------------------------------------------------/
!/
	END SUBROUTINE W3EXGI
!/
!/ End of W3GRID_INTERP -----------------------------------------------------/
!/
	END PROGRAM W3GRID_INTERP
